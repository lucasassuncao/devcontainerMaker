    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "DEBUG: create",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "create"
            ]
        },
        {
            "name": "DEBUG: add-extensions",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "add-extension",
                "--extension",
                "lucas.assuncao"
            ]
        },
        {
            "name": "DEBUG: add-settings",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "add-settings",
                "--key",
                "isLucas.Awesome",
                "--value",
                "true"
            ]
        },
        {
            "name": "DEBUG: add-features",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "add-features",
                "--key",
                "ghcr.io/lucasassuncao/features/test:1",
                "--value",
                "{\"enabled\": true}"
            ]
        }
    ]
}